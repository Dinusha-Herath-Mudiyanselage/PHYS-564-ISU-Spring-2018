   "source": [
    "def integrand(x):\n",
    "    return 1/(2+np.cos(x))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "ans,err = integrate.quad(integrand, 0, np.pi/2)"
